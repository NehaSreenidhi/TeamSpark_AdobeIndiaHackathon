    },
    {
      "level": "H1",
      "text": "1 lemon, sliced",
      "page": 3
    },
    {
      "level": "H1",
      "text": "1 small onion, sliced",
      "page": 3
    },
    {
      "level": "H1",
      "text": "1 zucchini, sliced",
      "page": 3
    },
    {
      "level": "H1",
      "text": "1/4 cup olive oil",
      "page": 3
    },
    {
      "level": "H1",
      "text": "1 tablespoon fresh dill, chopped",
      "page": 3
    },
    {
      "level": "H1",
      "text": "Salt and pepper to taste",
      "page": 3
    },
    {
      "level": "H1",
